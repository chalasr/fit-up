<?xml version="1.0" encoding="UTF-8"?>

<phpunit bootstrap="./app/bootstrap.php.cache" colors="true">
    <testsuites>
        <testsuite name="Exporter Test Suite">
            <directory suffix="Test.php">./test/Exporter/</directory>
        </testsuite>
    </testsuites>

    <filter>
        <whitelist>
            <directory suffix=".php">./src/*/*Bundle/Tests/Controller/*</directory>
        </whitelist>
    </filter>
</phpunit>
